{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular4client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/resources/css/bootstrap.min.css",
        "assets/resources/css/font-awesome.min.css",
        "assets/resources/css/vendors.css",
        "assets/resources/css/style.min.css",
        "assets/resources/css/components.min.css",
        "assets/resources/slider-revolution/css/settings.css",
        "assets/resources/slider-revolution/css/layers.css",
        "assets/resources/slider-revolution/css/navigation.css"
      ],
      "scripts": [
        "assets/resources/js/jquery.min.js",
        "assets/resources/js/popper.min.js",
        "assets/resources/js/bootstrap.min.js",
        "assets/resources/js/parallax.js",
        "assets/resources/slider-revolution/js/jquery.themepunch.tools.min.js",
        "assets/resources/slider-revolution/js/jquery.themepunch.revolution.min.js",
        "assets/resources/slider-revolution/js/extensions/revolution.extension.parallax.min.js",
        "assets/resources/slider-revolution/js/extensions/revolution.extension.video.min.js",
        "assets/resources/slider-revolution/js/extensions/revolution.extension.slideanims.min.js",
        "assets/resources/slider-revolution/js/extensions/revolution.extension.navigation.min.js",
        "assets/resources/slider-revolution/js/extensions/revolution.extension.layeranimation.min.js",
        "assets/resources/js/jquery.countdown.min.js",
        "assets/resources/js/jquery.countTo.js",
        "assets/resources/js/owl.carousel.min.js",
        "assets/resources/js/jquery.appear.js",
        "assets/resources/js/jquery.prettyPhoto.js",
        "assets/resources/js/main.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
